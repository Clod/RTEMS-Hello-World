{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "STM32 Debug via Mac ST-LINK",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/arm-rtems6-nucleo-h743zi/hello.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/opt/rtems/6.1/bin/arm-rtems6-gdb",
            "miDebuggerServerAddress": "host.docker.internal:61234",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
